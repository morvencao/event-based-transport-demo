{
    "clusterName": "cluster1",
    "spec": {
        "apiVersion": "apps/v1",
        "kind": "Deployment",
        "metadata": {
            "name": "nginx",
            "namespace": "default"
        },
        "spec": {
            "replicas": 1,
            "selector": {
                "matchLabels": {
                    "app": "nginx"
                }
            },
            "template": {
                "metadata": {
                    "labels": {
                        "app": "nginx"
                    }
                },
                "spec": {
                    "containers": [
                        {
                            "image": "quay.io/morvencao/nginx-unprivileged",
                            "name": "nginx"
                        }
                    ]
                }
            }
        }
    }
}